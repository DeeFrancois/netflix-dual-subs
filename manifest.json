{
    "manifest_version": 2,
  
    "name": "LLSubsNetflix",
    "description": "This extension will add subtitles in a target language to Netflix",
    "version": "1.6",
    "background": {
        "scripts": ["jquery-3.5.1.min.js","background.js"]
    },
  
    "permissions": [
        "storage","webNavigation","https://*.netflix.com/*"
      ],

    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"

    }

  }