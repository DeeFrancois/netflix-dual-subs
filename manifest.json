{
    "manifest_version": 2,
  
    "name": "NETFLIX Language Learning Subtitles",
    "description": "This extension will enable dual language subtitles for Netflix",
    "version": "0.9",
    "background": {
        "scripts": ["jquery-3.5.1.min.js","background.js"]
    },
  
    "permissions": [
        "storage","webNavigation","https://www.netflix.com/*"
      ],

    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"

    }

  }